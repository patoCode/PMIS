<div class="x_panel">
	<div class="x_title">
		<h2>Crear variable<small>formulario de creacion de variabes</small></h2>
		<ul class="nav navbar-right panel_toolbox">
			<li>
				<a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
			</li>
			<li>
				<a class="close-link"><i class="fa fa-close"></i></a>
			</li>
		</ul>
		<div class="clearfix"></div>
	</div>
	<div class="x_content">
		<br />
		<form class="form-horizontal form-label-left" method="POST" action="/variable/create">
			<div class="form-group row ">
				<label class="control-label col-md-3 col-sm-3 ">Nombre</label>
				<div class="col-md-9 col-sm-9 ">
					<input type="text" class="form-control" placeholder="Nombre" name="nombre" />
				</div>
			</div>
			<div class="form-group row ">
				<label class="control-label col-md-3 col-sm-3 ">Descripcion</label>
				<div class="col-md-9 col-sm-9 ">
					<input type="text" class="form-control" placeholder="Descripcion" name="descripcion" />
				</div>
			</div>
			<div class="form-group row ">
				<label class="control-label col-md-3 col-sm-3 ">Codigo</label>
				<div class="col-md-9 col-sm-9 ">
					<input type="text" class="form-control" placeholder="Codigo" name="codigo" />
				</div>
			</div>

			<div class="form-group row ">
				<label class="control-label col-md-3 col-sm-3 ">Orden</label>
				<div class="col-md-9 col-sm-9 ">
					<input type="number" class="form-control" placeholder="Orden" name="orden" />
				</div>
			</div>
			<%- include('../partials/status-element')%>

			<div class="ln_solid"></div>
			<div class="form-group">
				<div class="col-lg-12 col-md-12 col-sm-12">
					<button type="submit" class="btn btn-success">Procesar</button>
					<button type="reset" class="btn btn-primary">Limpiar</button>

				</div>
			</div>
		</form>
	</div>
</div>